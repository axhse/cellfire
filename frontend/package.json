{
  "name": "cellfire-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "katex": "0.16.21",
    "ol": "10.3.1",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-katex": "3.0.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "prettier": "^3.4.2",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.{js,jsx,json,css}\"",
    "build": "react-scripts build",
    "pack": "prettier --write \"src/**/*.{js,jsx,json,css}\" && react-scripts build",
    "start": "react-scripts start",
    "start:standalone": "cross-env REACT_APP_BUILD_NAME=standalone react-scripts start"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
